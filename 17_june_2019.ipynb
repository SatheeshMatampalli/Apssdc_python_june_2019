    "def highrem(n): # 5\n",
    "    r = 0\n",
    "    for i in range(1,n):\n",
    "        rem = n%i #5%1=0,5%2=1,5%3=2,5%4=1\n",
    "        if rem>r:#1>0 2>1 1!>=2\n",
    "            r = rem #r=1 r=2\n",
    "            j = i #j=2 j=3\n",
    "    return j\n",
    "\n",
    "t = int(input())\n",
    "for i in range(t):\n",
    "    n = int(input())\n",
    "    print(highrem(n))"
   ]
